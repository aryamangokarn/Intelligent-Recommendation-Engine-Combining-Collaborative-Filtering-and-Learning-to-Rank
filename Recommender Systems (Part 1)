    {
      "cell_type": "code",
      "source": [
        "from sklearn.model_selection import KFold"
      ],
      "metadata": {
        "id": "FC8GqTOWS22E"
      },
      "execution_count": 94,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def predict(test,user_mean_ratings ):\n",
        "    # Predict ratings using mean rating for each user\n",
        "    predictions = []\n",
        "    rmse_list = []\n",
        "\n",
        "    for user_id, item_id, rating in test[['userId', 'movieId', 'rating']].values:\n",
        "        if user_id in user_mean_ratings:\n",
        "            predictions.append(user_mean_ratings[user_id])\n",
        "        else:\n",
        "            # If user is not in training set, use a default value or skip the prediction\n",
        "            predictions.append(0)\n",
        "\n",
        "    # Evaluate the predictions (e.g., calculate RMSE)\n",
        "    actual_ratings = test['rating'].values\n",
        "    rmse = np.sqrt(np.mean((np.array(actual_ratings) - np.array(predictions)) ** 2))\n",
        "\n",
        "    # Append RMSE to the list\n",
        "    rmse_list.append(rmse)\n",
        "\n",
        "    # Calculate the average RMSE across all folds\n",
        "    avg_rmse = np.mean(rmse_list)\n",
        "    return (avg_rmse)\n",
        "    #print(\"Average RMSE:\", avg_rmse)"
      ],
      "metadata": {
        "id": "37h445N_QkhC"
      },
      "execution_count": 95,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "ratings = ratings_df"
      ],
      "metadata": {
        "id": "e0DxQuhpZeg2"
      },
      "execution_count": 96,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#For untrimmed data\n",
        "kf = KFold(n_splits = 10)\n",
        "\n",
        "ratings.head()\n",
        "\n",
        "avg_rmse_values_Naive_not_trimmed = []\n",
        "for train_indices, test_indices in kf.split(ratings):\n",
        "    train = ratings.iloc[train_indices]\n",
        "    test = ratings.iloc[test_indices]\n",
        "    user_mean_ratings = train.groupby('userId')['rating'].mean()\n",
        "    # Split the DataFrame into training and testing sets\n",
        "    mean_rating_filter = predict(test,user_mean_ratings)\n",
        "    avg_rmse_values_Naive_not_trimmed.append(mean_rating_filter)\n",
        "#print(avg_rmse_values_Naive_not_trimmed)\n",
        "print(\"Average RMSE across the 10 folds is:\", np.mean(avg_rmse_values_Naive_not_trimmed))"
      ],
