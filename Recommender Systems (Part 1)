        "user_ratings_count = ratings_df['userId'].value_counts()\n",
        "\n",
        "# Sort the users by the number of ratings in decreasing order\n",
        "sorted_users = user_ratings_count.sort_values(ascending=False)\n",
        "\n",
        "# Plot the distribution\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.plot(sorted_users.values, marker='o', linestyle='-', color='b')\n",
        "\n",
        "# Customize the plot\n",
        "plt.title('Distribution of Ratings Among Users')\n",
        "plt.xlabel('User Index (Ordered by Decreasing Frequency)')\n",
        "plt.ylabel('Number of Movies Rated')\n",
        "plt.grid(axis='y', linestyle='--', alpha=0.7)\n",
        "\n",
        "# Show the plot\n",
        "plt.show()"
      ],
      "metadata": {
