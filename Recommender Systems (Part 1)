    {
      "cell_type": "code",
      "source": [
        "# Assuming the dataset follows the structure mentioned in the question\n",
        "ratings_values = ratings_df['rating']\n",
        "\n",
        "# # Define bin edges\n",
        "# bin_edges = [i for i in range(1, 6)]  # Bins from 1 to 5 with width 1\n",
        "# bin_edges.append(6)  # Add an extra bin edge for ratings equal to 5\n",
        "\n",
        "# Define bin edges with width 0.5\n",
        "bin_edges = np.arange(0.5, 6, 0.5)\n",
        "\n",
        "# Plot the histogram\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.hist(ratings_values, bins=bin_edges, edgecolor='black', alpha=0.7)\n",
        "\n",
        "# Customize the plot\n",
        "plt.title('Histogram of Movie Ratings')\n",
        "plt.xlabel('Rating')\n",
        "plt.ylabel('Frequency')\n",
        "plt.xticks(bin_edges + 0.5)\n",
        "plt.grid(axis='y', linestyle='--', alpha=0.7)\n",
        "\n",
        "# Show the plot\n",
        "plt.show()\n"
      ],
