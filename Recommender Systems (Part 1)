    {
      "cell_type": "code",
      "source": [
        "# Assuming the dataset follows the structure mentioned in the question\n",
        "m = ratings_df['userId'].nunique()  # Number of users\n",
        "n = ratings_df['movieId'].nunique()  # Number of movies\n",
        "\n",
        "# Calculate the total number of possible ratings and the total number of available ratings\n",
        "total_possible_ratings = m * n"
      ],
      "metadata": {
        "id": "hq2veeIDXiYZ"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "available_ratings = ratings_df[['userId']].count().sum()\n",
        "\n",
        "# Calculate sparsity\n",
        "sparsity = available_ratings / total_possible_ratings\n",
        "\n",
        "# Print the sparsity\n",
        "print(f\"Sparsity: {sparsity}\")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "6xpj5Y4qXnzE",
        "outputId": "2ff5c64f-8cd9-43d8-ff3d-784d92283db9"
      },
      "execution_count": 7,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Sparsity: 0.016999683055613623\n"
          ]
        }
      ]
    },
