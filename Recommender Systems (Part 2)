        "## Question 15\n"
      ],
      "metadata": {
        "id": "MonnlJkAXU15"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def get_top_features(model, top_n=5):\n",
        "    feature_names = model.feature_name()\n",
        "    importance = model.feature_importance(importance_type='gain')\n",
        "\n",
        "    # Create a dictionary of feature names and their importance scores\n",
        "    feature_importance_dict = dict(zip(feature_names, importance))\n",
        "\n",
        "    # Get the top N features based on importance scores\n",
        "    top_features = sorted(feature_importance_dict.items(), key=lambda x: x[1], reverse=True)[:top_n]\n",
        "\n",
        "    return top_features"
      ],
      "metadata": {
        "id": "eOMf97odXPvk"
      },
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Loop through each fold and print the top 5 most important features\n",
        "for fold_number in range(1, 6):\n",
        "    print(\"------------------------------------------------------------------------------------------------\")\n",
        "\n",
        "    print(f\"For Fold {fold_number}:\")\n",
        "    data_path = f\"/content/drive/MyDrive/219_LSDM/Project3/MSLR-WEB10K/Fold{fold_number}/\"\n",
        "    # Load data for the fold\n",
        "    X_train, y_train, qid_train, group_train, X_test, y_test, qid_test, group_test = load_one_fold(data_path)\n",
        "\n",
        "    # Train the LightGBM model\n",
        "    model = train_lgb_model(X_train, y_train, qid_train, group_train)\n",
        "\n",
        "    # Get the top 5 most important features for the fold\n",
        "    top_features = get_top_features(model, top_n=5)\n",
        "\n",
